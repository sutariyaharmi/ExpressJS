<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Todo List</title>
</head>
<body>
    <h1>todo list</h1>
    <div>
        <table>
            <thead>
                <tr>
                    <th>firstName</th>
                    <th>lastName</th>
                    <th>email</th>
                    <th>MobileNo</th>
                </tr>
            </thead>
            <tbody>
                <% users.forEach(function(user) { %>
                    <tr>
                         <td><%= user.firstName %></td>  
                         <td><%= user.lastName %></td>  
                         <td><%= user.email %></td>  
                         <td><%= user.mobileNo %></td>  
                    </tr>
                    <% }); %>
            </tbody>
        </table>
    </div>
</body>
</html>